<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="CDOWin.Views.Clients.Dialogs.UpdateNotes"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dialogs="using:CDOWin.Views.Clients.Dialogs"
    mc:Ignorable="d">

    <Grid Width="480" RowSpacing="10" ColumnSpacing="12">
        <!-- Row 0: Notes -->
        <TextBox Grid.Row="0" Grid.Column="0"
                 Style="{StaticResource MultiLineTextBoxStyle}"
                 Header="New Note" TextChanged="LabeledMultiLinePair_TextChanged"/>
    </Grid>
</Page>
